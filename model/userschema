
const mongoose =require('mongoose')

const userSchema = new mongoose.userSchema({

    name:{
        type:String,
        required:true,
    },
    email:{
        type:String,
        required:true,
        unique:true
        },
    number:{
        type:Number,
        required:true,
        unique:true,
        },
        age:{
            type:string,
            positive : true,
        },
})

const userModel = mongoose.model("user",userSchema)
model.export =userSchema
